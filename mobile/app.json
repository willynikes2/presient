{
  "expo": {
    "name": "Presient",
    "slug": "presient",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "presient",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.presient"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0f172a"
      },
      "package": "com.yourname.presient"
    },
    "plugins": [
      "expo-secure-store",
      "expo-web-browser"
    ]
  }
}